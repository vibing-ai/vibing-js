{
  "name": "@vibing-ai/sdk",
  "version": "1.0.0",
  "description": "Vibing AI SDK for creating AI-powered apps, plugins, and agents",
  "author": "Vibing AI Team",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "docs/project/CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    },
    "./app": {
      "require": "./dist/app/index.js",
      "import": "./dist/app/index.esm.js",
      "types": "./dist/app/index.d.ts"
    },
    "./plugin": {
      "require": "./dist/plugin/index.js",
      "import": "./dist/plugin/index.esm.js",
      "types": "./dist/plugin/index.d.ts"
    },
    "./agent": {
      "require": "./dist/agent/index.js",
      "import": "./dist/agent/index.esm.js",
      "types": "./dist/agent/index.d.ts"
    },
    "./core": {
      "require": "./dist/core/index.js",
      "import": "./dist/core/index.esm.js",
      "types": "./dist/core/index.d.ts"
    }
  },
  "keywords": [
    "vibing",
    "ai",
    "sdk",
    "artificial intelligence",
    "conversational",
    "agent",
    "plugin",
    "app"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vibing-ai/js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/vibing-ai/js-sdk/issues"
  },
  "homepage": "https://github.com/vibing-ai/js-sdk#readme",
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "npm test -- --findRelatedTests"
    ]
  },
  "scripts": {
    "build": "rimraf dist && rollup -c rollup.config.cjs",
    "test": "jest --config jest.config.cjs",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "prepare": "husky",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "postpublish": "git tag v$npm_package_version && git push --tags",
    "update-deps": "npx npm-check-updates -u",
    "install-peers": "npm install --no-save github:vibing-ai/vibing-block-kit"
  },
  "peerDependencies": {
    "@vibing-ai/block-kit": "^1.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "@vibing-ai/block-kit": {
      "optional": true
    }
  },
  "dependencies": {
    "intersection-observer": "^0.12.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "semver": "^7.5.4",
    "tslib": "^2.5.0",
    "whatwg-fetch": "^3.6.20"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.21.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4",
    "webpack-bundle-analyzer": "^4.8.0"
  },
  "overrides": {
    "@testing-library/react-hooks": {
      "@types/react": "^18.0.0"
    },
    "@testing-library/react": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all",
    "last 2 versions"
  ]
}
